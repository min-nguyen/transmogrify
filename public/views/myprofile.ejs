    <!-- Header script to reload js on embedded pages -->
    <script language="text/javascript">
        function load_js()
        {
           var head= document.getElementsByTagName('head')[0];
           var script= document.createElement('script');
           script.type= 'text/javascript';
           script.src= '../javascripts/myprofile.js';
           head.appendChild(script);
        }
        load_js();
    </script>
</head>
<body class = "profile_body">
    <div class = "profile_page_container">
        <div id="profile_title_container"><h1 class="profile_title"> My <span> Profile </span></h1></div>

        <section class="profile_container">
            <div class = "profile_card_wrapper">
                <figure class="profile_card" id="profile_card" >
                    <div class="ACon profile_profile">
                    
                        <div class="profile_img" id = "profile_image1">
                            <span></img></span>
                        </div>
                        <div class="profile_img" id = "profile_image2">
                                <div class="profile_upload-image-container 
                                            profile_upload-image-clearfix">
                                    <div class="profile_upload-image-avatar"
                                         style="background-image:url('<%=profile_picture%>');"
                                         onclick="document.getElementById('profile_put_file').click()">
                                            <svg id="profile_upload-image-camera" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
                                                viewBox="0 0 25 15" enable-background="new 0 0 25 15" xml:space="preserve">
                                            <path id="profile_upload-image-cameraFrame" fill="none" stroke="white" stroke-miterlimit="10" d="M23.1,14.1H1.9c-0.6,0-1-0.4-1-1V1.9c0-0.6,0.4-1,1-1h21.2
                                                c0.6,0,1,0.4,1,1v11.3C24.1,13.7,23.7,14.1,23.1,14.1z"/>
                                            <path id="profile_upload-image-circle" fill="none" stroke="#ffffff" stroke-width="1.4" stroke-miterlimit="12" d="M17.7,7.5c0-2.8-2.3-5.2-5.2-5.2S7.3,4.7,7.3,7.5s2.3,5.2,5.2,5.2
                                                S17.7,10.3,17.7,7.5z"/>
                                            <g id="profile_upload-image-plus">
                                                <path fill="none" id="profile_upload-image-plusLine" class="profile_upload-image-line" stroke="#ffffff" stroke-linecap="round" stroke-miterlimit="10" d="M20.9,2.3v4.4"/>
                                                <path fill="none" class="profile_upload-image-line"stroke="#ffffff" stroke-linecap="round" stroke-miterlimit="10" d="M18.7,4.6h4.4"/>
                                            </g>
                                            </svg>
                                    </div>
                                </div>
                                <!-- <input id="profile_put_file"  type="file" /> -->
                        </div>
                            <input id="profile_put_file"  type="file" disabled />
                        </div>
                        <figcaption id="profile_profile-description">
                            <div class="profile_profile-public">
                                <h3 class="profile_name" id="profile_name"><%=firstname%> <%=surname%></h3>
                                <h3>What I do
                                    <div  id="profile_occupation_pencil" class="glyphicon glyphicon-pencil"></div>
                                    
                                    <span id="profile_occupation">
                                        <div id="occupation_box">
                                            <div id="profile_occupation_text">
                                                <%=occupation%>
                                            </div>
                                        </div>
                                    </span>
                                </h3>
                                <h3>About Me
                                    <div  id="profile_description_pencil" class="glyphicon glyphicon-pencil"></div>
                                    <span id="profile_aboutme">
                                        <div id="description_box">
                                            <div id="profile_description_text">
                                                <%=description%>
                                            </div>
                                        </div>
                                    </span>
                                </h3>
                                <i class="flip-profile-icon icon-flipped fa fa-share" aria-hidden="true"></i>
                            </div>
                            <div class="profile_profile-settings">
                                <h5 class="profile_profile-settings-type">Name</h5>
                                <h5 id="profile_settings_name" class="profile_profile-settings-value"><%=firstname%> <%=surname%></h5>
                                <h5 class="profile_profile-settings-type">Email Address</h5>
                                <h5 "profile_settings_email"  class="profile_profile-settings-value" id="profile_email"><%=email%></h5>
                                <h5 class="profile_profile-settings-type"><%=occupation%></h5>
                                <h5 class="profile_profile-settings-value">Likes & Listens</h5>
                                <br>
                                <h5 class="profile_profile-settings-edit">RESET PASSWORD</h5>
                                <h5 class="profile_profile-settings-edit">EDIT NAME </h5>
                                <i class="flip-profile-icon icon-flipped fa fa-share" aria-hidden="true"></i>
                            </div>
                        </figcaption>
                    </div>
                </figure>
            </div>
        </section>
        
        
        <div class= "profile_gallery-wrapper">
            <div class= "profile_gallery-container">
                <div class="brick-wall">

                    <!-- IDEAL BRICK -->
                    <figure class="brick" 
                        data-username   = ""
                        data-date       = ""
                        data-file-id    = "">
                        <div class = "brick-img-audio-container">
                            <img  class = "brick-img" src="http://iris.theaureview.com/wp-content/uploads/2015/04/it-follows-film.png">
                            <div class = "brick-audio"> 
                            </div>
                        </div>
                        <div class="player">
                            <div class="control-panel">
                                <div class="album-art brick_profile_img">
                                </div>
                                <div class="info-bar">
                                    <span class="artist">Flume</span>
                                    <span class="name">Say it</span>
                                    <div class="progress-bar">
                                        <div class="bar"></div>
                                    </div>
                                </div>
                                <div class="controls">
                                    <div class="play-container pause">
                                        <i class="fa fa-play play" aria-hidden="true">
                                                
                                        </i> 
                                    </div>
                                </div>
                            </div>
                        </div>
                    </figure>
                    <!-- IDEAL BRICK -->
    
                    <figure class="brick" 
                        data-username   = ""
                        data-date       = ""
                        data-file-id    = "">
                        <div class = "brick-img-audio-container">
                            <img  class = "brick-img" src="http://iris.theaureview.com/wp-content/uploads/2015/04/it-follows-film.png">
                            <div class = "brick-audio"> 
                            </div>
                        </div>
                        <div class="player">
                            <div class="control-panel">
                                <div class="album-art brick_profile_img">
                                    
                                </div>
                                <div class="info-bar">
                                    <span class="artist">Flume</span>
                                    <span class="name">Say it</span>
                                    <div class="progress-bar">
                                        <div class="bar"></div>
                                    </div>
                                </div>
                                <div class="controls">
                                    <div class="play-container pause">
                                        <i class="fa fa-play play" aria-hidden="true">
                                                
                                        </i> 
                                    </div>
                                </div>
                            </div>
                        </div>
                    </figure>
                    
                    
                </div>
            </div>  
        </div>
    </div>
